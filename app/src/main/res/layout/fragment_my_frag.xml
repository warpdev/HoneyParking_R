<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".my_frag">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_marginTop="36dp"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:id="@+id/topbarmy">

        <ImageView
            android:id="@+id/imageButtonbackmy"
            android:layout_width="9.5dp"
            android:layout_height="17.5dp"
            android:background="@drawable/join_icn_back_copy" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="24.5dp"
            android:textSize="17sp"
            android:textColor="#422814"
            android:lineSpacingExtra="18.8sp"
            android:fontFamily="@font/nanumsquareb"
            android:text="내정보"
            />
    </LinearLayout>
    <ImageView
        android:layout_width="40.5dp"
        android:layout_height="29dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="28dp"
        android:layout_below="@id/topbarmy"
        android:id="@+id/carimg"
        android:background="@mipmap/logo_car"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/carinfobar"
        android:layout_toRightOf="@id/carimg"
        android:layout_below="@id/topbarmy">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#422814"
            android:lineSpacingExtra="17.8sp"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="9dp"
            android:fontFamily="@font/nanumsquareeb"
            tools:text="홍길동"
            />
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="#a09288"
        android:lineSpacingExtra="17.8sp"
        android:layout_marginTop="25dp"
            android:layout_marginLeft="3dp"
            android:fontFamily="@font/nanumsquarer"
        tools:text="님의 차"
        />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#422814"
            android:lineSpacingExtra="17.8sp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="25dp"
            tools:text="56아 2906"
            android:fontFamily="@font/nanumsquareeb"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/minibar"
        android:layout_below="@id/carinfobar"
        android:layout_toRightOf="@id/carimg">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="#422814"
            android:lineSpacingExtra="23.8sp"
            tools:text="총 주차횟수 23회"
            android:fontFamily="@font/nanumsquareb"
            android:layout_marginLeft="9dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="#d9d9d9"
            android:lineSpacingExtra="23.8sp"
            android:layout_marginLeft="10dp"
            android:fontFamily="@font/nanumsquareb"
            tools:text="|"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9.5dp"
            android:lineSpacingExtra="23.8sp"
            android:textColor="#f7c633"
            android:textSize="12sp"
            android:fontFamily="@font/nanumsquareeb"
            tools:text="정보 변경하기  &gt;" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@id/minibar">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_weight="1"
        android:layout_below="@id/minibar"
        android:layout_marginTop="21dp"
        android:gravity="center_vertical"
        android:background="@drawable/sqr_top_bot">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="#422814"
            android:lineSpacingExtra="21.8sp"
            android:layout_marginLeft="16dp"
            android:fontFamily="@font/nanumsquareb"
            android:text="결제"
            />
    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:background="@drawable/sq_line_bot">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#422814"
                android:lineSpacingExtra="21.8sp"
                android:layout_marginLeft="16dp"
                android:fontFamily="@font/nanumsquareb"
                android:text="쿠폰함"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:background="@drawable/sq_line_bot">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#422814"
                android:lineSpacingExtra="21.8sp"
                android:layout_marginLeft="16dp"
                android:fontFamily="@font/nanumsquareb"
                android:text="즐겨찾기"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:background="@drawable/sq_line_bot">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#422814"
                android:lineSpacingExtra="21.8sp"
                android:layout_marginLeft="16dp"
                android:fontFamily="@font/nanumsquareb"
                android:text="환경설정"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:background="@drawable/sq_line_bot">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textColor="#422814"
                android:lineSpacingExtra="21.8sp"
                android:layout_marginLeft="16dp"
                android:fontFamily="@font/nanumsquareb"
                android:text="문의하기"
                />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>